<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="https://jman001188.github.io/CSE445Assignment4/Hotels.xsd" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <element name="Hotels">
    <complexType>
      <all>
        <element name="Hotel" minOccurs="1" maxOccurs="unbounded">
          <complexType>
            <sequence>
              <element name="Name" type="string" minOccurs="1" maxOccurs="1" />
              <element name="Phone" type="string" minOccurs="1" maxOccurs="unbounded" />
              <element name="Address" minOccurs="1" maxOccurs="1">
                <complexType>
                  <sequence>
                    <element name="Number" type="integer" minOccurs="1" maxOccurs="1" />
                    <element name="Street" type="string" minOccurs="1" maxOccurs="1" />
                    <element name="City" type="string" minOccurs="1" maxOccurs="1" />
                    <element name="State" type="string" minOccurs="1" maxOccurs="1" />
                    <element name="Zip" type="integer" minOccurs="1" maxOccurs="1" />
                  </sequence>
                  <attribute name="NearestAirport" use="required" />
                </complexType>
              </element>
            </sequence>
            <attribute name="Rating" type="decimal" use="optional" />
          </complexType>
        </element>
      </all>
    </complexType>
  </element>
</schema>
    
                  
                  
        
        
      
